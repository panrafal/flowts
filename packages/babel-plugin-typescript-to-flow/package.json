{
  "name": "babel-plugin-typescript-to-flow",
  "description": "Babel plugin to convert TypeScript into Flow",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.0.1",
  "scripts": {
    "prepare": "yarn run build",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc -b",
    "watch": "tsc --watch",
    "cover": "jest --coverage",
    "report-coverage": "codecov"
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/babel-plugin-typescript-to-flow",
  "peerDependencies": {
    "@babel/core": "^7.4.4"
  },
  "dependencies": {
    "@babel/template": "^7.8.3",
    "@babel/traverse": "^7.8.4",
    "@babel/types": "7.9.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "7.0.9",
    "@types/jest": "^25.2.1",
    "@zxbodya/recast": "^0.18.9",
    "codecov": "^3.6.5",
    "jest": "^25.2.7",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
