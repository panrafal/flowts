{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noEmitOnError": false,
  }
}
